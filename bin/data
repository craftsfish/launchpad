#!/bin/bash
dir="/tmp"
today=$(date +%Y%m%d)
for f in $(ls ${dir}/*.csv); do
	r=$(echo $f | grep "^ExportOrderList${today}.*$")
	if [[ $r != "" ]] ; then
		mv $r ${dir}/tm.list.csv
	fi

	r=$(echo $f | grep "^ExportOrderDetailList${today}.*$")
	if [[ $r != "" ]] ; then
		mv $r ${dir}/tm.detail.csv
	fi
done
